{
  "name": "turtlevale-functions",
  "description": "Cloud Functions for Turtlevale",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^1.3.4",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.8.0",
    "node-cron": "^3.0.2",
    "stripe": "^11.16.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/react": "^17.0.87",
    "typescript": "^4.9.5"
  },
  "private": true
}
